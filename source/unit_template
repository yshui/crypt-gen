Before=cryptsetup.target
{{#block?}}
BindsTo={{device}}
After={{device}}
{{/block?}}
{{#wants}}
After={{dep}}
Wants={{dep}}
{{/wants}}
{{#teardown?}}
Conflicts=initrd-switch-root.target
{{/teardown?}}

Before=umount.target

[Service]
Type=oneshot
RemainAfterExit=yes
TimeoutSec=0
KeyringMode=shared
OOMScoreAdjust=500
ExecStart=/usr/lib/systemd/systemd-cryptsetup attach '{{name}}' '{{device_file}}' '{{key}}' '{{options}}'
ExecStop=/usr/lib/systemd/systemd-cryptsetup detach '{{name}}' {{#teardown?}}; /usr/bin/rm '{{device_file}}' {{/teardown?}}
